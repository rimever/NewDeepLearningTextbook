    "# 変数yearに西暦で年を入力してください\n",
    "year = 2000\n",
    "\n",
    "# if文により条件分岐をし、うるう年か平年かを判別します\n",
    "if not(year % 400 > 0 and year % 100 == 0) and year % 4 == 0:\n",
    "    print(year,\"年はうるう年です\")\n",
    "else:\n",
    "    print(year,\"年は平年です \")"
